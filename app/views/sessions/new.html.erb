<div class ="page-header">	
	<h3>Log in</h3>
</div>

	<%= form_tag sessions_path do %>
	  <p>
	    <%= label_tag :username %>
	    <%= text_field_tag :username, params[:email] %>
	  </p>
	  <p>
	    <%= label_tag :password %>
	    <%= password_field_tag :password %>
	  </p>
	  <%= submit_tag "Log In", :class => 'btn' %>
	<% end %>
